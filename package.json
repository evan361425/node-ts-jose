    "@types/node": "^18.0",
    "@types/sinon": "^10.0",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "chai": "^4.3",
    "eslint": "^8",
    "eslint-config-prettier": "^8",
    "eslint-plugin-mocha": "^10",
    "eslint-plugin-prettier": "^4",
    "mocha": "^10.0",
    "nyc": "^15.1",
    "prettier": "^2",
    "rimraf": "^4",
    "sinon": "^15.0",
    "source-map-support": "^0.5",
    "ts-node": "^10.7",
    "typescript": "^4.6"
  },
  "engines": {
    "node": ">=12"
  }
}
