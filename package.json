    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "chai": "^4.3.0",
    "eslint": "^7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^9.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=12"
  }
