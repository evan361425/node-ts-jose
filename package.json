  "keywords": [
    "JOSE",
    "helper",
    "wrapper",
    "TypeScript",
    "interface",
    "class based"
  ],
  "homepage": "https://github.com/evan361425/ts-jose#readme",
  "bugs": {
    "url": "https://github.com/evan361425/ts-jose/issues"
  },
  "repository": {
